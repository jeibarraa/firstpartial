#Function 1
def name():
  print("¿Can you tell me the price of the hamburger?")
  hamburger=input()
  print("what is the discount?")
  discount=input()
  price=100-int(discount)
  total=(int(hamburger)/100)*price
  print("the total price would be of:", total)

#Function 2
name()


